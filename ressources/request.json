{
  "get_time_bdd": {
    "aggs": {
      "min_date": {
        "min": {
          "field": "pub_date",
          "format": "yyyy-MM-dd"
        }
      },
      "max_date": {
        "max": {
          "field": "pub_date",
          "format": "yyyy-MM-dd"
        }
      }
    }
  },
  "get_top_ten_categorie": {
    "query": {
      "match_all": {}
    },
    "aggs": {
      "group_by_categories": {
        "categorize_text": {
          "field": "categories"
        }
      }
    }
  },
  "get_last_news": {
    "size": 10,
    "sort": [
      {
        "pub_date": {
          "order": "desc",
          "format": "strict_date_optional_time_nanos"
        }
      }
    ]
  },
  "get_count_filter": {
        "query": {
            "bool": {
                "must": [
                    {
                        "match": {
                            "Titres": ""
                        }
                    }
                ],
                "filter": [
                    {
                        "range": {
                            "pub_date": {
                                "gte": "2020-01-01",
                                "lte": "2020-01-25"
                            }
                        }
                    }
                ]
            }
        },
        "aggs": {
            "group_by_date": {
                "date_histogram": {
                    "field": "pub_date",
                    "calendar_interval": "day",
                    "format": "yyyy-MM-dd"
                }
            }
        }
    }
}
